@model IEnumerable<DraGLand.Models.CarStore>

@{
    ViewBag.Title = "Store";
}

@Html.Partial("Partial")

<div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="false">

    <div class="carousel-inner">
        <div class="item active">
            <img src="~/images/cars/vaz1.png" alt="VAZ">
            <button>10.000</button>
        </div>
        <div class="item">
            <img src="~/images/cars/bmw_1series(1).png" alt="BMW">
            <button>20.000</button>
        </div>
        <div class="item">
            <img src="~/images/cars/mercedes1(1).png" alt="Mercedes">
            <button>30.000</button>
        </div>
        <div class="item">
            <img src="~/images/cars/porsche_911(1).png" alt="Porsche">
            <button>40.000</button>
        </div>
        <div class="item">
            <img src="~/images/cars/audi_r8(1).png" alt="Audi">
            <button>50.000</button>
        </div>
        <div class="item">
            <img src="~/images/cars/ferrari_f12(1).png" alt="Ferrari">
            <button>60.000</button>
        </div>


    </div>

    <a class="left carousel-control" href="#myCarousel" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right"></span>
        <span class="sr-only">Next</span>
    </a>
</div>






